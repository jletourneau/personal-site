<% icon_build_dir = '../../../assets/jlet-iconfont/build' %>

@font-face {
  font-family: jlet;
  font-display: swap;
  src: inline-font-files("<%= icon_build_dir %>/jlet.woff", woff);
}

.i:before {
  font: normal normal normal 100%/1 jlet;
  speak: none;
  text-transform: none;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

@import "<%= icon_build_dir %>/codepoints";
